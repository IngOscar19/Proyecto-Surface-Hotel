						{
							"listen": "test",
							"script": {
								"exec": [
									"// Extraer el token del cuerpo de la respuesta y guardarlo en una variable de colección",
									"if (pm.response.code === 200) {",
									"    var jsonData = pm.response.json();",
									"    if (jsonData.token) {",
									"        pm.collectionVariables.set(\"token\", jsonData.token);",
									"        pm.test(\"Token almacenado en variable de colección\", function () {",
									"            pm.expect(pm.collectionVariables.get(\"token\")).to.eql(jsonData.token);",
									"        });",
									"    } else {",
									"        pm.test(\"La respuesta no contiene la propiedad 'token'\", function () {",
									"            pm.expect(jsonData.token).to.exist;",
									"        });",
									"    }",
									"} else {",
									"    pm.test(\"Respuesta no exitosa\", function () {",
